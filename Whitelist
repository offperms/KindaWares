

     local colors = {
    SchemeColor = Color3.fromRGB(230, 35, 69),
    Background = Color3.fromRGB(48, 48, 48),
    Header = Color3.fromRGB(24, 24, 24),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(32, 32, 32)
}


local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KindaWare Loader", colors)


local Load = Window:NewTab("KindaWare")
local Load = Load:NewSection("KindaWare Loader")

Load:NewButton("Classic Theme", "Loads KindaWare Classic", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/KindaWare/KindaWare/main/Classic%20Theme"))()
end)

Load:NewButton("Light Theme", "Loads KindaWare Light", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/KindaWare/KindaWare/main/Light%20Theme"))()
end)
